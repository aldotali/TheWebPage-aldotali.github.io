import java.util.*;

public class Lab06c
{
    public boolean compareString( ArrayList<String> x)
    {
        
        if (x.size() <= 1)
            return true;
        else if ( x.get(1).compareTo(x.get(0)) < 0)
        { 
            return false;
        }
        else 
        {
            x.remove(0);
            return compareString((x));
        }
    }
}